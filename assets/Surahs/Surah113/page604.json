[
  {"ayahNumber": 0, "arabicAyah": "بسم الله الرحمن الرحيم", "somaliTranslation": "Magaca Eebe yaan kubillaabaynaa ee Naxariis guud iyo mid gaaraba Naxariista"},
  {"ayahNumber": 1, "arabicAyah": "قُلْ أَعُوذُ بِرَبِّ الْفَلَقِ", "somaliTranslation": "Waxaad dhahdaa (Nabiyow) waxaan ka magangalay Eebeha Subaxa (abuuray)"},
  {"ayahNumber": 2, "arabicAyah": "مِن شَرِّ مَا خَلَقَ", "somaliTranslation": "Wixii shar leh sharkiisa"},
  {"ayahNumber": 3, "arabicAyah": "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ", "somaliTranslation": "Iyo Habeenka sharkiisa markuu madoobaado"},
  {"ayahNumber": 4, "arabicAyah": "وَمِن شَرِّ النَّفَّاثَاتِ فِي الْعُقَدِ", "somaliTranslation": "Iyo sharka Sixirrowga wax ku tufa guntimaha"},
  {"ayahNumber": 5, "arabicAyah": "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ", "somaliTranslation": "Iyo sharka Xaasidka wax Xasda"}
