[
  {"ayahNumber": 0, "arabicAyah": "بسم الله الرحمن الرحيم", "somaliTranslation": "Magaca Eebe yaan kubillaabaynaa ee Naxariis guud iyo mid gaaraba Naxariista"},
  {"ayahNumber": 1, "arabicAyah": "أَلَمْ نَشْرَحْ لَكَ صَدْرَكَ", "somaliTranslation": "Miyannaan kuu waasicinin Laabta (ku nuurinin)"},
  {"ayahNumber": 2, "arabicAyah": "وَوَضَعْنَا عَنكَ وِزْرَكَ", "somaliTranslation": "Oonaan ka dajinin culayskii"},
  {"ayahNumber": 3, "arabicAyah": "الَّذِي أَنقَضَ ظَهْرَكَ", "somaliTranslation": "dhabarka kaa cusleeyay"},
  {"ayahNumber": 4, "arabicAyah": "وَرَفَعْنَا لَكَ ذِكْرَكَ", "somaliTranslation": "Miyaannaan kor yeelin magacaaga (xusiddaada)"},
  {"ayahNumber": 5, "arabicAyah": "فَإِنَّ مَعَ الْعُسْرِ يُسْرًا", "somaliTranslation": "Cidhiidhina waxaa ku dhaw fudayd (faraj)"},
  {"ayahNumber": 6, "arabicAyah": "إِنَّ مَعَ الْعُسْرِ يُسْرًا", "somaliTranslation": "Cidhiidhina waxaa ku dhaw fudayd"},
