[
  {"ayahNumber": 0, "arabicAyah": "بسم الله الرحمن الرحيم", "somaliTranslation": "Magaca Eebe yaan kubillaabaynaa ee Naxariis guud iyo mid gaaraba Naxariista"},
  {"ayahNumber": 1, "arabicAyah": "الْقَارِعَةُ", "somaliTranslation": "Qaaricada (qalbiga garaacda)"},
  {"ayahNumber": 2, "arabicAyah": "مَا الْقَارِعَةُ", "somaliTranslation": "Qaarica maxay tahay"},
  {"ayahNumber": 3, "arabicAyah": "وَمَا أَدْرَاكَ مَا الْقَارِعَةُ", "somaliTranslation": "Mase taqaannaa Qaarico"},
  {"ayahNumber": 4, "arabicAyah": "يَوْمَ يَكُونُ النَّاسُ كَالْفَرَاشِ الْمَبْثُوثِ", "somaliTranslation": "Waa Maalinta Dadku noqon sida Baalallay"},
  {"ayahNumber": 5, "arabicAyah": "وَتَكُونُ الْجِبَالُ كَالْعِهْنِ الْمَنفُوشِ", "somaliTranslation": "Buuruhuna ay noqon sida Suuf kala firdhay"},
  {"ayahNumber": 6, "arabicAyah": "فَأَمَّا مَن ثَقُلَتْ مَوَازِينُهُ", "somaliTranslation": "Ruuxiise miisaankiisa (fiican) cuslaado"},
  {"ayahNumber": 7, "arabicAyah": "فَهُوَ فِي عِيشَةٍ رَّاضِيَةٍ", "somaliTranslation": "Wuxuu ku noolaan nolol raalli ah"},
  {"ayahNumber": 8, "arabicAyah": "وَأَمَّا مَنْ خَفَّتْ مَوَازِينُهُ", "somaliTranslation": "Ruuxiise uu fududaado Miisaankiisa (fiican)"},
  {"ayahNumber": 9, "arabicAyah": "فَأُمُّهُ هَاوِيَةٌ", "somaliTranslation": "Wuxuu u hoydaa (Naarta) Haawiya"},
  {"ayahNumber": 10, "arabicAyah": "وَمَا أَدْرَاكَ مَا هِيَهْ", "somaliTranslation": "Ma taqaannaa Haawiya"},
  {"ayahNumber": 11, "arabicAyah": "نَارٌ حَامِيَةٌ", "somaliTranslation": "Waa Naar kulayl daran"}
]