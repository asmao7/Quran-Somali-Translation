[
  {"ayahNumber": 0, "arabicAyah": "بسم الله الرحمن الرحيم", "somaliTranslation": "Magaca Eebe yaan kubillaabaynaa ee Naxariis guud iyo mid gaaraba Naxariista"},
  {"ayahNumber": 1, "arabicAyah": "قُلْ يَا أَيُّهَا الْكَافِرُونَ", "somaliTranslation": "Waxaad dhahdaa, Gaalooy"},
  {"ayahNumber": 2, "arabicAyah": "لَا أَعْبُدُ مَا تَعْبُدُونَ", "somaliTranslation": "Ma caabudayo waxaad caabudaysaan"},
  {"ayahNumber": 3, "arabicAyah": "وَلَا أَنتُمْ عَابِدُونَ مَا أَعْبُدُ", "somaliTranslation": "Idinna ma caabudaysaan Eebahaan caabudi"},
  {"ayahNumber": 4, "arabicAyah": "وَلَا أَنَا عَابِدٌ مَّا عَبَدتُّمْ", "somaliTranslation": "Mana caabudin waxaad caabuddeen"},
  {"ayahNumber": 5, "arabicAyah": "وَلَا أَنتُمْ عَابِدُونَ مَا أَعْبُدُ", "somaliTranslation": "Idinkuna (hadda) ma caabudaysaan Eebahaan caabudi"},
  {"ayahNumber": 6, "arabicAyah": "لَكُمْ دِينُكُمْ وَلِيَ دِينِ", "somaliTranslation": "Idinkana Diintiina yaad leedihiin, Anna Diintaydaan leeyahay"}
