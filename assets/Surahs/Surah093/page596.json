[
  {"ayahNumber": 0, "arabicAyah": "بسم الله الرحمن الرحيم", "somaliTranslation": "Magaca Eebe yaan kubillaabaynaa ee Naxariis guud iyo mid gaaraba Naxariista"},
  {"ayahNumber": 1, "arabicAyah": "وَالضُّحَى", "somaliTranslation": "Eebe wuxuu ku dhaartay Barqinka"},
  {"ayahNumber": 2, "arabicAyah": "وَاللَّيْلِ إِذَا سَجَى", "somaliTranslation": "Iyo Habeenku markuu xasilo (Madoobaado)"},
  {"ayahNumber": 3, "arabicAyah": "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَى", "somaliTranslation": "Inaan Eebe ka tagin Nabiyow kuuna cadhoonin"},
  {"ayahNumber": 4, "arabicAyah": "وَلَلْآخِرَةُ خَيْرٌ لَّكَ مِنَ الْأُولَى", "somaliTranslation": "Aakharana kaaga khayr badan Aduunka"},
  {"ayahNumber": 5, "arabicAyah": "وَلَسَوْفَ يُعْطِيكَ رَبُّكَ فَتَرْضَى", "somaliTranslation": "Eebaana ku siindoona (wanaag) waadna raalli noqon"},
  {"ayahNumber": 6, "arabicAyah": "أَلَمْ يَجِدْكَ يَتِيمًا فَآوَى", "somaliTranslation": "Miyuusan ku helin Eebahaa Adigoo Agoon ah oosan ku dhawaynin"},
  {"ayahNumber": 7, "arabicAyah": "وَوَجَدَكَ ضَالًّا فَهَدَى", "somaliTranslation": "Miyuunase ku helin Adigoo baadi ah oosan ku hanuuninin"},
  {"ayahNumber": 8, "arabicAyah": "وَوَجَدَكَ عَائِلًا فَأَغْنَى", "somaliTranslation": "Miyuunase ku helin Adigoo caydhi ah oosna ku hodminin (kuu waxyoonin)"},
  {"ayahNumber": 9, "arabicAyah": "فَأَمَّا الْيَتِيمَ فَلَا تَقْهَرْ", "somaliTranslation": "Ee Agoonta ha xoogin (hana dhihin)"},
  {"ayahNumber": 10, "arabicAyah": "وَأَمَّا السَّائِلَ فَلَا تَنْهَرْ", "somaliTranslation": "Ka wax su'aalina ha canaanan"},
  {"ayahNumber": 11, "arabicAyah": "وَأَمَّا بِنِعْمَةِ رَبِّكَ فَحَدِّثْ", "somaliTranslation": "Nicmada Eebahaana ka sheekee (ku mahadi)"}
]