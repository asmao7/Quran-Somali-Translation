[
  {"ayahNumber": 0, "arabicAyah": "أَرَأَيْتَ الَّذِي يُكَذِّبُ بِالدِّينِ", "somaliTranslation": "Magaca Eebe yaan kubillaabaynaa ee Naxariis guud iyo mid gaaraba Naxariista"},
  {"ayahNumber": 1, "arabicAyah": "بسم الله الرحمن الرحيم", "somaliTranslation": "Ka warrama kan beenin Abaalmarinta (Qiyaamada)"},
  {"ayahNumber": 2, "arabicAyah": "فَذَلِكَ الَّذِي يَدُعُّ الْيَتِيمَ", "somaliTranslation": "Kaas xooga Agoonta (xageeda)"},
  {"ayahNumber": 3, "arabicAyah": "وَلَا يَحُضُّ عَلَى طَعَامِ الْمِسْكِينِ", "somaliTranslation": "Aan kuna boorrinaynin quudinta miskiinka"},
  {"ayahNumber": 4, "arabicAyah": "فَوَيْلٌ لِّلْمُصَلِّينَ", "somaliTranslation": "Waxaa halaag u sugnaaday kuwa tukan (waa Munaafiqiinta)"},
  {"ayahNumber": 5, "arabicAyah": "الَّذِينَ هُمْ عَن صَلَاتِهِمْ سَاهُونَ", "somaliTranslation": "Ee haddana halmaansan Salaaddooda"},
  {"ayahNumber": 6, "arabicAyah": "الَّذِينَ هُمْ يُرَاؤُونَ", "somaliTranslation": "Dadkana is tusiya"},
  {"ayahNumber": 7, "arabicAyah": "وَيَمْنَعُونَ الْمَاعُونَ", "somaliTranslation": "Una diida alaabta (Laysugu kaalmeeyo)"}
]