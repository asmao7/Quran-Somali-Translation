[
  {"ayahNumber": 0, "arabicAyah": "بسم الله الرحمن الرحيم", "somaliTranslation": "Magaca Eebe yaan kubillaabaynaa ee Naxariis guud iyo mid gaaraba Naxariista"},
  {"ayahNumber": 1, "arabicAyah": "وَيْلٌ لِّكُلِّ هُمَزَةٍ لُّمَزَةٍ", "somaliTranslation": "Halaag Wuxuu u Sugnaaday xanle wax ceebeya oo Dhan"},
  {"ayahNumber": 2, "arabicAyah": "الَّذِي جَمَعَ مَالًا وَعَدَّدَهُ", "somaliTranslation": "Oo Xoolaha kulmiya (xaqana ku bixinin)"},
  {"ayahNumber": 3, "arabicAyah": "يَحْسَبُ أَنَّ مَالَهُ أَخْلَدَهُ", "somaliTranslation": "Ma wuxuu malayn in xoolihiisu waarin"},
  {"ayahNumber": 4, "arabicAyah": "كَلَّا لَيُنبَذَنَّ فِي الْحُطَمَةِ", "somaliTranslation": "Saas ma aha ee waxaa lagu tuuri (Naarta) Xudhama"},
  {"ayahNumber": 5, "arabicAyah": "وَمَا أَدْرَاكَ مَا الْحُطَمَةُ", "somaliTranslation": "Ma taqaannaa Xudhama"},
  {"ayahNumber": 6, "arabicAyah": "نَارُ اللَّهِ الْمُوقَدَةُ", "somaliTranslation": "Waa Naarta Eebe ee la huriyay"},
  {"ayahNumber": 7, "arabicAyah": "الَّتِي تَطَّلِعُ عَلَى الْأَفْئِدَةِ", "somaliTranslation": "Gubaysana Quluubta (Kulayl dartiis)"},
  {"ayahNumber": 8, "arabicAyah": "إِنَّهَا عَلَيْهِم مُّؤْصَدَةٌ", "somaliTranslation": "Waana lagu dabooli Naartaas Gaalada"},
  {"ayahNumber": 9, "arabicAyah": "فِي عَمَدٍ مُّمَدَّدَةٍ", "somaliTranslation": "Tiraar la fidiyay yayna ahaan dhexdeeda"}
]